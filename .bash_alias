# Path variables
alias 'cdgit'='cd ~/git'
alias 'cdconf'='cd ~/git-conf'


# Scripts
alias 'temp'='bash ~/git-conf/scripts/temp.sh'
alias 'push'='bash ~/git-conf/scripts/pusher.sh'
alias 'branch'='bash ~/git-conf/scripts/branch.sh'
alias 'clone'='bash ~/git-conf/scripts/clone.sh'
alias 'newtoken'='bash ~/git-conf/scripts/setToken.sh'
alias 'install'='bash ~/git-conf/install.sh'
alias 'uninstall'='bash ~/git-conf/uninstall.sh && history -c'
alias 'initiate'='bash ~/git-conf/install.sh && source ~/.bash_profile && cd ~/git'


# Useful alias
alias 'cls'='clear'
alias 'md'='mkdir'
alias 'copy'='cp'
alias 'ralias'='source ~/.bash_alias'
alias 'token'='cat ~/temp/token.txt'
alias 'gatinha'='cat'


# Git alias
alias gcl='git clone --depth 1'
alias gi='git init'
alias ga='git add'
alias gc='git commit -m'
alias gp='git push -u origin main'
alias gt='git status'
alias status='git status'